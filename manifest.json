{
  "manifest_version": 3,
  "name": "Weshet Filler",
  "version": "0.1.0",
  "description": "A lightweight extension that helps developers, testers, and users quickly fill out online forms with relevant, Ethiopia-specific information.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Fill all your forms with Weshet Filler"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["data.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
